import java.util.*;
class area
{
    int l,b;
public void getdata()
{
Scanner sc=new Scanner(System.in);
System.out.println("enter values in l and h");
l=sc.nextInt();
b=sc.nextInt();
int c=l*b;
System.out.println("area is="+c);
}
public static void main(String arg[])
{
     area a1= new area();
     a1.getdata();
}
}
